<h1>Pages#indexPois</h1>

<% @pois.each do |poi| %>
<div>
  <%= poi.name %>
  <%= poi.address %>
  <%= poi.address2 %>
  <%= poi.zip_code %>
  <%= poi.city %>
  <%= poi.pk %>
  <%= poi.latitude %>
  <%= poi.longitude %>
  <%= poi.way_id %>
  <%= link_to "Show", way_poi_path(params[:way_id], poi.slug) %>
  <%= link_to "Edit", edit_way_poi_path(params[:way_id], poi.slug)%>
  <%= link_to "Delete", way_poi_path(params[:way_id], poi.slug), method: :delete %>
</div>
<% end %>
